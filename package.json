{
  "name": "strapi-blurhash-plugin",
  "version": "0.1.0",
  "description": "A Strapi Plugin to generate blurhash for each uploaded images",
  "keywords": [
    "strapi",
    "plugin",
    "media",
    "blurhash",
    "thumbhash"
  ],
  "type": "commonjs",
  "exports": {
    "./package.json": "./package.json",
    "./strapi-server": {
      "types": "./dist/server/src/index.d.ts",
      "source": "./server/src/index.ts",
      "import": "./dist/server/index.mjs",
      "require": "./dist/server/index.js",
      "default": "./dist/server/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "strapi-plugin build",
    "watch": "strapi-plugin watch",
    "watch:link": "strapi-plugin watch:link",
    "verify": "strapi-plugin verify",
    "test:ts:back": "run -T tsc -p server/tsconfig.json"
  },
  "dependencies": {
    "node-fetch": "^3.3.2",
    "sharp": "^0.33.5",
    "thumbhash": "^0.1.1"
  },
  "devDependencies": {
    "@strapi/sdk-plugin": "^5.3.0",
    "@strapi/strapi": "^5.6.0",
    "@strapi/typescript-utils": "^5.6.0",
    "prettier": "^3.4.2",
    "typescript": "^5.7.2"
  },
  "peerDependencies": {
    "@strapi/sdk-plugin": "^5.3.0",
    "@strapi/strapi": "^5.6.0"
  },
  "strapi": {
    "name": "strapi-blurhash-plugin",
    "description": "A Strapi Plugin to generate blurhash for each uploaded images",
    "kind": "plugin",
    "displayName": "Strapi Thumbhash"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/AguzzTN54/strapi-blurhash-plugin.git"
  },
  "homepage": "https://github.com/AguzzTN54/strapi-blurhash-plugin",
  "author": {
    "name": "AguzzTN54",
    "url": "https://github.com/AguzzTN54"
  },
  "contributors": [
    {
      "name": "Emil Petras",
      "url": "https://github.com/emil-petras"
    }
  ]
}
